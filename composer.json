{
    "name": "gentor/activecampaign",
    "description": "Wrapper on the ActiveCampaign PHP API - with custom methods and support for Laravel 5.x",
    "keywords": [
        "laravel",
        "activecampaign",
        "api"
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "JeraNS/activecampaign-api-php",
                "version": "1.0",
                "type": "package",
                "source": {
                    "url": "https://github.com/jeraNS/activecampaign-api-php.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Evgeni Nurkov",
            "email": "evgeni.nurkov@gmail.com"
        }
    ],
    "require": {
        "php": ">= 5.6",
        "JeraNS/activecampaign-api-php": "1.0",
    },
    "autoload": {
        "psr-4": {
            "Gentor\\ActiveCampaign\\": "src/"
        }
    },
    "minimum-stability": "dev"
}